<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="paused" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>

  <include file="$(find racer_world)/launch/racer_gazebo.launch">
      <arg name="world" value="racetrack_decorated_fast_sim"/>
      <arg name="paused" value="$(arg paused)"/>
      <arg name="use_sim_time" value="true"/>
      <arg name="gui" value="$(arg gui)"/>
      <arg name="headless" value="$(arg headless)"/>
      <arg name="debug" value="false"/>
      <arg name="verbose" value="false"/>
      <arg name="use_gpu" value="true"/>
  </include>

  <include file="$(find vesc_sim)/launch/vesc_sim.launch"/>

  <include file="$(find car_tf)/launch/car_transformer.launch"/>

  <include file="$(find car_control)/launch/car_control.launch">
      <arg name="mode_override" value="2"/>
  </include>

  <!-- Q-Learning related nodes: -->

  <!--node
    pkg="q_learning"
    type="learn.py"
    name="learn"
    output="screen" >
  </node-->

  <node
    pkg="q_learning"
    type="crash_detector"
    name="crash_detector"
    output="screen" >
  </node>
</launch>
